{
  "name": "Chrome Tab Saver",
  
  "version": "1.0",
  
  "description": "Chrome extension save and load tab sets.",
  
  "browser_action": {
    "default_popup": "popup.html"
	},

  "manifest_version": 2,
  "incognito": "split",
  
  "permissions": [
     "activeTab",
     "tabs",
     "file://*/*"
   ],

  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" }
}